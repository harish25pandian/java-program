import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int x = n / 10;
        int p = n % 10;

        int sm = x + p;
        System.out.println("Bird said:" + n);
        System.out.println("Alice must go in path-" + sm);
    }
}
